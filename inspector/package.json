{
  "name": "simply-mcp-inspector",
  "version": "0.3.0",
  "description": "Web-based inspector for exploring and testing Model Context Protocol (MCP) servers",
  "private": false,
  "type": "module",
  "bin": {
    "simply-mcp-inspector": "./dist/cli.js",
    "simplymcp-inspector": "./dist/cli.js"
  },
  "files": [
    "dist",
    ".next/standalone",
    ".next/static",
    "public",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "next dev",
    "dev:local": "SIMPLY_MCP_CLI_PATH=../dist/src/cli/run-bin.js next dev",
    "build": "npm run build:next && npm run post-build && npm run build:cli",
    "build:next": "next build --webpack",
    "post-build": "node scripts/post-build.js",
    "build:cli": "tsc --project tsconfig.cli.json",
    "start": "next start",
    "start:local": "SIMPLY_MCP_CLI_PATH=../dist/src/cli/run-bin.js node dist/cli.js",
    "lint": "next lint",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Clockwork-Innovations/simply-mcp-ts.git",
    "directory": "inspector"
  },
  "keywords": [
    "mcp",
    "inspector",
    "model-context-protocol",
    "debugging",
    "development-tools",
    "simply-mcp"
  ],
  "author": {
    "name": "Nicholas Marinkovich, MD",
    "url": "https://cwinnov.com"
  },
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.544.0",
    "next": "^16.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "ws": "^8.18.3",
    "yargs": "^18.0.0"
  },
  "peerDependencies": {
    "simply-mcp": "^4.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "@types/node": "^24.9.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/ws": "^8.18.1",
    "@types/yargs": "^17.0.33",
    "eslint": "^9.38.0",
    "eslint-config-next": "^16.0.1",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "simply-mcp": "file:..",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3"
  }
}
